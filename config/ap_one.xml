<analysis>
  <!-- start with analysis parameters and switches -->
  <parameters>
    <despikeSigma> 15.0 </despikeSigma>
    <lowpassFilterKnee> 8.0 </lowpassFilterKnee>
    <timeOffset> 0.125 </timeOffset>
    <timeChunk> 0 </timeChunk>
    <cutStd> 0 </cutStd>
    <neigToCut> 3 </neigToCut>
    <splineOrder> 0 </splineOrder>
    <tOrder> 0 </tOrder>
    <cleanPixelSize> 8 </cleanPixelSize>
    <cleanStripe> 1 </cleanStripe>
    <controlChunk> 0.01 </controlChunk>
    <resample> 1.0 </resample>
    <approximateWeights> 1 </approximateWeights>
    <masterGridJ2000_0> 0.0 </masterGridJ2000_0>
    <masterGridJ2000_1> 0.0 </masterGridJ2000_1>
    <pixelSize> 1.5 </pixelSize>
    <noiseMapsPerObs> 0 </noiseMapsPerObs>
    <azelMap> 1 </azelMap>
    <threadNumber> 1 </threadNumber>
    <cutTurnArounds> 1 </cutTurnArounds>
    <cutSamplesAtEndOfScans> 40 </cutSamplesAtEndOfScans>
    <writeBeammapToNcdf> 1 </writeBeammapToNcdf>
  </parameters>

  <!-- observation filelist -->
  <observations>
    <rawDataPath>input/</rawDataPath>
    <bsPath>input/</bsPath>
    <outBeammapInfoPath>output/</outBeammapInfoPath>
    <outBeammapNcdfPath>output/</outBeammapNcdfPath>
    <nFiles>1</nFiles>
    <f0>
      <fileName>53701.nc</fileName>
      <bsName>53701.bstats</bsName>
      <outBeammapInfoName>53701_out.bstats</outBeammapInfoName>
      <outBeammapNcdfName>53701_out.fits</outBeammapNcdfName>
      <beammapSourceFlux> 3.45 </beammapSourceFlux>
    </f0>
  </observations>
</analysis>
